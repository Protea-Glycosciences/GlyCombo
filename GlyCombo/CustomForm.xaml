        <Label Content="Ranges" Margin="620,119,0,0" FontWeight="Bold" Height="19" VerticalAlignment="Top" HorizontalAlignment="Left" Width="48"/>
        <TextBox x:Name="customMonoMinBox1" Margin="619,163,0,0" local:NumericInputBehavior.IsNumericInput="True" Width="65" MinWidth="65" Height="32" VerticalAlignment="Top" HorizontalAlignment="Left" FontSize="12" MaxLength="2" Text="0"/>
