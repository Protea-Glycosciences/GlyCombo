        <TextBox x:Name="customMonoNameBox4" Margin="63,271,0,0" TextWrapping="Wrap" Text="Custom Name 4" HorizontalAlignment="Left" Width="129" Height="32" VerticalAlignment="Top" FontSize="12"/>
