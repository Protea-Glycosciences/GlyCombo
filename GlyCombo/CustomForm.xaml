        <TextBox x:Name="customMonoHBox2" Margin="404,199,0,0" local:NumericInputBehavior.IsNumericInput="True" Width="65" MinWidth="65" HorizontalAlignment="Left" Height="32" VerticalAlignment="Top" FontSize="12" Padding="5,5,1,6" Text="0"/>
        <TextBox x:Name="customMonoNBox2" Margin="473,199,0,0" local:NumericInputBehavior.IsNumericInput="True" Width="65" MinWidth="65" Height="32" VerticalAlignment="Top" HorizontalAlignment="Left" FontSize="12" Padding="5,5,1,6" Text="0"/>
        <TextBox x:Name="customMonoOBox2" Margin="543,199,0,0" local:NumericInputBehavior.IsNumericInput="True" Width="65" MinWidth="65" HorizontalAlignment="Left" Height="32" VerticalAlignment="Top" FontSize="12" Padding="5,5,1,6" Text="0"/>
