        <TextBox x:Name="customMonoNBox1" Margin="473,163,0,0" local:NumericInputBehavior.IsNumericInput="True" MinWidth="65" Height="32" VerticalAlignment="Top" HorizontalAlignment="Left" Width="65" FontSize="12" Padding="5,5,0,6" Text="0"/>
