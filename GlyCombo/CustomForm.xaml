<Window x:Class="GlyCombo.CustomForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:glycombo"
        mc:Ignorable="d"
        Title="Custom Monosaccharides" Height="450" Width="800" MinWidth="800">

    <Grid Margin="-1,0,1,0" MaxHeight="440" MinWidth="800">
        <TextBlock x:Name="customText" Margin="10,10,10,324" TextWrapping="Wrap" Text="TextBlock" MaxHeight="100"/>
        <Label Content="Custom Monosaccharides" Margin="10,115,0,0" FontWeight="Bold" Height="19" VerticalAlignment="Top" HorizontalAlignment="Left" Width="168"/>
        <CheckBox x:Name="customMonoCheck1" Content="1" Margin="10,163,0,0" VerticalAlignment="Top" Width="17" MinWidth="50" Checked="customMonoCheck1_Checked" Unchecked="customMonoCheck1_Unchecked" Height="32"/>
        <CheckBox x:Name="customMonoCheck2" Content="2" Margin="10,198,0,0" VerticalAlignment="Top" Width="17" MinWidth="50" Checked="customMonoCheck2_Checked" Unchecked="customMonoCheck2_Unchecked" Height="32"/>
        <CheckBox x:Name="customMonoCheck3" Content="3" Margin="10,233,0,0" VerticalAlignment="Top" Width="17" MinWidth="50" Checked="customMonoCheck3_Checked" Unchecked="customMonoCheck3_Unchecked" Height="32"/>
        <CheckBox x:Name="customMonoCheck4" Content="4" Margin="10,269,0,0" VerticalAlignment="Top" Width="17" MinWidth="50" Checked="customMonoCheck4_Checked" Unchecked="customMonoCheck4_Unchecked" Height="32"/>
        <CheckBox x:Name="customMonoCheck5" Content="5" Margin="10,304,0,0" VerticalAlignment="Top" Width="17" MinWidth="50" Checked="customMonoCheck5_Checked" Unchecked="customMonoCheck5_Unchecked" Height="32"/>
        <Label Content="Enable" Margin="10,139,0,0" Height="19" VerticalAlignment="Top" HorizontalAlignment="Left" Width="41"/>
        <TextBox x:Name="customMonoNameBox1"  Margin="63,163,0,0" TextWrapping="Wrap" Text="Custom Name 1" Height="32" VerticalAlignment="Top" HorizontalAlignment="Left" Width="129" FontSize="12"/>
        <Label Content="Custom Name" HorizontalAlignment="Left" Margin="63,139,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="customMonoMassBox1" local:NumericInputBehavior.IsNumericInput="True" Margin="203,163,0,0" TextWrapping="Wrap" HorizontalAlignment="Left" Width="124" Height="32" VerticalAlignment="Top" FontSize="12"  Text="0"/>
        <Label Content="Monoisotopic mass" HorizontalAlignment="Left" Margin="203,139,0,0" VerticalAlignment="Top"/>
        <Button x:Name="customSettingsSave" Content="Export settings" Margin="10,356,0,0" VerticalAlignment="Top" Click="customSettingsSave_Click" Width="149"/>
        <Button x:Name="customSettingsLoad" Content="Import settings" Margin="176,356,0,0" VerticalAlignment="Top" Click="customSettingsLoad_Click" Width="151"/>
        <Label Content="Chemical formula" Margin="334,119,0,0" FontWeight="Bold" Height="19" VerticalAlignment="Top" HorizontalAlignment="Left" Width="116"/>
        <TextBox x:Name="customMonoCBox1" Margin="334,163,0,0" local:NumericInputBehavior.IsNumericInput="True" MinWidth="62" Height="32" VerticalAlignment="Top" HorizontalAlignment="Left" Width="66" FontSize="12" Padding="5,5,0,6" Text="0"/>
        <Label Content="C" Margin="334,142,0,0" Height="19" VerticalAlignment="Top" HorizontalAlignment="Left" Width="9"/>
        <Label Content="H" Margin="406,143,0,0" Height="19" VerticalAlignment="Top" HorizontalAlignment="Left" Width="10"/>
