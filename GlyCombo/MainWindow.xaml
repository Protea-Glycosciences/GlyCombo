<Window x:Class="glycombo.MainWindow"
        xmlns:ui="http://schemas.modernwpf.com/2019"
        ui:WindowHelper.UseModernWindowStyle="True"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:glycombo"
        mc:Ignorable="d"
        Title="GlyCombo 0.4 - Protea Glycosciences" Height="648" Width="800" Icon="/GlyCombo_Logo_Final.png" MinHeight="655" MinWidth="800">


    <Grid Margin="3,3,7,3" RenderTransformOrigin="0.43,0.449">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto" />
            <ColumnDefinition MinWidth="200" />
            <ColumnDefinition MinWidth="450" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="195" />
            <RowDefinition Height="Auto" MinHeight="117.277" />
            <RowDefinition Height="Auto" MinHeight="52" />
            <RowDefinition Height="Auto" MinHeight="84" />
            <RowDefinition Height="Auto" MinHeight="182" />

        </Grid.RowDefinitions>
        <Label Margin="10,10,10,10" Style="{StaticResource heading}">Input:</Label>
        <StackPanel Grid.Row="0" Grid.Column="1"  Margin="10,10,10,10">
            <RadioButton GroupName="Upload" Name="TextRadioButton" Grid.Row="0" Grid.Column="1" IsChecked="True" Checked="TextRadioButton_Checked">Text</RadioButton>
            <RadioButton GroupName="Upload" Name="MzmlRadioButton" Grid.Row="0" Grid.Column="1" Checked="MzmlRadioButton_Checked">mzML</RadioButton>
            <ui:LayoutPanel x:Name="TextInput" Margin="0,10,0,0" >
                <TextBox x:Name="InputMasses" Grid.Row="0" Height="65" Width="175" HorizontalAlignment="Left" PreviewTextInput="NumberValidationTextBox" ui:ControlHelper.PlaceholderText="Insert list of masses" VerticalScrollBarVisibility="Auto" TextWrapping="Wrap" AcceptsReturn="True"></TextBox>
                <ui:LayoutPanel.Style>
                    <Style TargetType="ui:LayoutPanel">
                        <Setter Property="Visibility" Value="Collapsed"/>
                        <Style.Triggers>
                            <DataTrigger Binding="{Binding IsChecked, ElementName=TextRadioButton}" Value="True">
                                <Setter Property="Visibility" Value="Visible"/>
                            </DataTrigger>
                        </Style.Triggers>
                    </Style>
                </ui:LayoutPanel.Style>
            </ui:LayoutPanel>
            <ui:LayoutPanel>
                <Button Grid.Row="2" Content="Browse File" Margin="0,10,0,10" Click="UploadButton_Click"></Button>
                <ui:LayoutPanel.Style>
                    <Style TargetType="ui:LayoutPanel">
                        <Setter Property="Visibility" Value="Collapsed"/>
                        <Style.Triggers>
                            <DataTrigger Binding="{Binding IsChecked, ElementName=MzmlRadioButton}" Value="True">
                                <Setter Property="Visibility" Value="Visible"/>
                            </DataTrigger>
                        </Style.Triggers>
                    </Style>
                </ui:LayoutPanel.Style>
            </ui:LayoutPanel>
        </StackPanel>
        <Label Style="{StaticResource heading}" HorizontalAlignment="Center" Width="114" Height="80" VerticalAlignment="Top" Margin="0,9,0,0" Grid.Row="1">Mass Error:</Label>
        <StackPanel Grid.Column="1" Margin="10,0,10,0" Height="100" VerticalAlignment="Center" Grid.Row="1">
            <ui:LayoutPanel>
                <TextBox x:Name="DaError" Grid.Row="1" Grid.ColumnSpan="2" Text="0.6" PreviewTextInput="NumberValidationTextBox" HorizontalAlignment="Left" TextAlignment="Right" Width="175"/>
                <ui:LayoutPanel.Style>
                    <Style TargetType="ui:LayoutPanel">
                        <Setter Property="Visibility" Value="Collapsed"/>
                        <Style.Triggers>
                            <DataTrigger Binding="{Binding IsChecked, ElementName=Da}" Value="True">
                                <Setter Property="Visibility" Value="Visible"/>
                            </DataTrigger>
                        </Style.Triggers>
                    </Style>
                </ui:LayoutPanel.Style>
            </ui:LayoutPanel>
            <ui:LayoutPanel>
                <TextBox x:Name="ppmError" Grid.Row="1" Grid.ColumnSpan="2" Text="50" HorizontalAlignment="Left" TextAlignment="Right" Width="175"/>
                <ui:LayoutPanel.Style>
                    <Style TargetType="ui:LayoutPanel">
                        <Setter Property="Visibility" Value="Collapsed"/>
                        <Style.Triggers>
                            <DataTrigger Binding="{Binding IsChecked, ElementName=ppm}" Value="True">
                                <Setter Property="Visibility" Value="Visible"/>
                            </DataTrigger>
                        </Style.Triggers>
                    </Style>
                </ui:LayoutPanel.Style>
            </ui:LayoutPanel>
            <RadioButton GroupName="Error" Name="Da" Grid.Row="1" Grid.Column="1" IsChecked="True">Da</RadioButton>
            <RadioButton GroupName="Error" Name="ppm" Grid.Row="1" Grid.Column="1">ppm</RadioButton>
        </StackPanel>
        <Label Grid.Row="2" Margin="10,0,10,0" Style="{StaticResource heading}" Height="32" VerticalAlignment="Center">Reducing End:</Label>
        <StackPanel Grid.Row="2" Grid.Column="1"  Margin="10,0,10,0" Height="32" VerticalAlignment="Center">
            <ComboBox x:Name="reducingEndBox" Width="171" SelectedIndex="0">
                <ComboBoxItem Content="Free"/>
                <ComboBoxItem Content="Reduced"/>
                <ComboBoxItem Content="InstantPC"/>
                <ComboBoxItem Content="Rapifluor-MS"/>
                <ComboBoxItem Content="2-aminobenzoic acid"/>
                <ComboBoxItem Content="2-aminobenzamide"/>
                <ComboBoxItem Content="Procainamide"/>
                <ComboBoxItem Content="Girard's reagent P"/>
            </ComboBox>
        </StackPanel>
        <Label Grid.Row="3" Margin="10,0,10,0" Style="{StaticResource heading}" Height="64" VerticalAlignment="Center">Glycan State:</Label>
        <StackPanel Grid.Row="3" Grid.Column="1" Margin="10,0,10,0" Height="64" VerticalAlignment="Center">
            <RadioButton GroupName="State" Name="Native" Grid.Row="3" Grid.Column="1" IsChecked="True">Native</RadioButton>
            <RadioButton GroupName="State" Name="Permeth" Grid.Row="3" Grid.Column="1" >Permethylated</RadioButton>
        </StackPanel>

        <Button x:Name="submitbutton" Grid.Row="4" Grid.Column="1" Content="Submit" MinWidth="175" IsEnabled="True" Click="Button1_Click" Margin="10,21,0,0" VerticalAlignment="Top" Height="31" Width="175"/>
        <ProgressBar x:Name="ProgressBarSubmit" Visibility="Collapsed"  Grid.Column="1" IsIndeterminate="True" HorizontalAlignment="Left" Height="8" Margin="10,54,0,0" Grid.Row="4" Width="175" RenderTransformOrigin="0.5,0.5" VerticalAlignment="Top"/>
        <Button x:Name="resetbutton" Grid.Row="4" Grid.Column="1" Content="Reset" MinWidth="175" IsEnabled="False" Click="Reset_Click" Margin="10,0,0,0" Height="32" Width="175"/>


        <Label Grid.Row="0" Grid.Column="2" Style="{StaticResource subheading}">Monosaccharides</Label>
        <Label Grid.Row="0" Grid.Column="2" Margin="0,0,17,1" Style="{StaticResource subheading}" HorizontalAlignment="Right" Width="35">Max</Label>
        <Label Grid.Row="0" Grid.Column="2" Margin="0,0,89,1" HorizontalAlignment="Right" Style="{StaticResource subheading}" Width="32">Min</Label>

        <ui:ToggleSwitch x:Name="HextoggleSwitch" Grid.Row="0" Grid.Column="2" Margin="0,20,0,0" VerticalAlignment="Top" OffContent="" OnContent="" Toggled="HextoggleSwitch_Toggled" Width="50" Height="32"/>
        <Label Grid.Row="0" Grid.Column="2" Margin="50,26,0,0" Height="20" VerticalAlignment="Top">Hexose (Hex)</Label>
        <ui:LayoutPanel x:Name="Hex_container" Grid.Row="0" Grid.Column="2" Margin="0,26,0,147" Visibility="Collapsed">
            <TextBox x:Name="HexMin" Grid.Row="0" Grid.RowSpan="5" PreviewTextInput="NumberValidationTextBox" Grid.Column="2" Text="0" MaxLength="2"  VerticalAlignment="Center" Margin="0,0,70,0" Width="50" Height="20" MinHeight="18" Padding="10,0,6,0" HorizontalAlignment="Right"/>
            <TextBox x:Name="HexMax" Grid.Row="0" Grid.RowSpan="5" PreviewTextInput="NumberValidationTextBox" Grid.Column="2" Text="0" MaxLength="2" VerticalAlignment="Center" Width="50" Height="20" MinHeight="18" Padding="10,0,6,0" HorizontalAlignment="Right"/>
        </ui:LayoutPanel>

        <ui:ToggleSwitch x:Name="HexNActoggleSwitch" Grid.Row="0" Grid.Column="2" Margin="0,46,0,0" VerticalAlignment="Top" OffContent="" OnContent="" Toggled="HexNActoggleSwitch_Toggled" Width="50" Height="32"/>
        <Label Grid.Row="0" Grid.Column="2" Margin="50,53,0,0" Height="20" VerticalAlignment="Top">N-acetyl-hexosamine (HexNAc)</Label>
        <ui:LayoutPanel x:Name="HexNAc_container" Grid.Row="0" Grid.Column="2" Margin="0,53,0,122" Visibility="Collapsed">
            <TextBox x:Name="HexNAcMin" Grid.Row="0" Grid.RowSpan="5" PreviewTextInput="NumberValidationTextBox" Grid.Column="2" Text="0" MaxLength="2" VerticalAlignment="Center" Margin="0,0,70,0" Width="50" Height="20" MinHeight="18" Padding="10,0,6,0" HorizontalAlignment="Right"/>
            <TextBox x:Name="HexNAcMax" Grid.Row="0" Grid.RowSpan="5" PreviewTextInput="NumberValidationTextBox" Grid.Column="2" Text="0" MaxLength="2" VerticalAlignment="Center" Width="50" Height="20" MinHeight="18" Padding="10,0,6,0" HorizontalAlignment="Right"/>
        </ui:LayoutPanel>

        <ui:ToggleSwitch x:Name="dHextoggleSwitch" Grid.Column="2" OffContent="" OnContent="" Toggled="dHextoggleSwitch_Toggled" Width="50" Height="32" Margin="0,72,0,0" VerticalAlignment="Top"/>
        <Label Grid.Column="2" Margin="50,77,0,0" Height="20" VerticalAlignment="Top">Deoxyhexose (dHex)</Label>
        <ui:LayoutPanel x:Name="dHex_container" Grid.Row="0" Grid.Column="2" Margin="0,77,0,96" Visibility="Collapsed">
            <TextBox x:Name="dHexMin" Grid.Row="0" Grid.RowSpan="5" PreviewTextInput="NumberValidationTextBox" Grid.Column="2" Text="0" MaxLength="2" VerticalAlignment="Center" Margin="0,0,70,0" Width="50" Height="20" MinHeight="18" Padding="10,0,6,0" HorizontalAlignment="Right"/>
            <TextBox x:Name="dHexMax" Grid.Row="0" Grid.RowSpan="5" PreviewTextInput="NumberValidationTextBox" Grid.Column="2" Text="0" MaxLength="2" VerticalAlignment="Center" Width="50" Height="20" MinHeight="18" Padding="10,0,6,0" HorizontalAlignment="Right"/>
        </ui:LayoutPanel>

        <ui:ToggleSwitch x:Name="Neu5ActoggleSwitch" Grid.Column="2" OffContent="" OnContent="" Toggled="Neu5ActoggleSwitch_Toggled" Width="50" Height="32" Margin="0,98,0,0" VerticalAlignment="Top"/>
        <Label Grid.Row="0" Grid.Column="2" Margin="50,104,0,0" Height="20" VerticalAlignment="Top">N-acetyl-neuraminic acid (NeuAc)</Label>
        <ui:LayoutPanel x:Name="NeuAc_container" Grid.Row="0" Grid.Column="2" Margin="0,104,0,42" Visibility="Collapsed">
            <TextBox x:Name="Neu5AcMin" Grid.Row="0" Grid.RowSpan="5" PreviewTextInput="NumberValidationTextBox" Grid.Column="2" Text="0" MaxLength="2" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,0,70,0" Width="50" Height="20" MinHeight="18" Padding="10,0,6,0"></TextBox>
            <TextBox x:Name="Neu5AcMax" Grid.Row="0" Grid.RowSpan="5" PreviewTextInput="NumberValidationTextBox" Grid.Column="2" Text="0" MaxLength="2" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,0,0,0" Width="50" Height="20" MinHeight="18" Padding="10,0,6,0"></TextBox>
        </ui:LayoutPanel>

        <ui:ToggleSwitch x:Name="Neu5GctoggleSwitch" Grid.Row="0" Grid.Column="2" Margin="0,124,0,0" VerticalAlignment="Top" OffContent="" OnContent="" Toggled="Neu5GctoggleSwitch_Toggled" Width="50" Height="32"/>
        <Label Grid.Row="0" Grid.Column="2" Margin="50,130,0,0" Height="20" VerticalAlignment="Top">N-glycolyl-neuraminic acid (NeuGc)</Label>
        <ui:LayoutPanel x:Name="NeuGc_container" Grid.Row="0" Grid.Column="2" Margin="0,129,0,17" Visibility="Collapsed">
            <TextBox x:Name="Neu5GcMin" Grid.Row="0" Grid.RowSpan="5" PreviewTextInput="NumberValidationTextBox" Grid.Column="2" Text="0" MaxLength="2" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,0,70,0" Width="50" Height="20" MinHeight="18" Padding="10,0,6,0"></TextBox>
            <TextBox x:Name="Neu5GcMax" Grid.Row="0" Grid.RowSpan="5" PreviewTextInput="NumberValidationTextBox" Grid.Column="2" Text="0" MaxLength="2" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,0,0,0" Width="50" Height="20" MinHeight="18" Padding="10,0,6,0"></TextBox>
        </ui:LayoutPanel>

        <Button x:Name="advanced_button" Grid.Column="2" VerticalAlignment="Top" Click="Advanced_Button" Margin="10,157,0,0" Padding="8,2,8,2" Height="27" Width="230">
            <TextBlock x:Name="advanced_text" Text="Show Advanced Monosaccharides"/>
        </Button>

        <ComboBox x:Name="PresetCombo" Grid.Column="2" Margin="290,157,0,0" ui:ControlHelper.PlaceholderText="Presets" FontSize="12" SelectionChanged="PresetCombo_SelectionChanged" Height="27" HorizontalAlignment="Stretch">
            <ComboBoxItem Content="N/A" FontSize="12"></ComboBoxItem>
            <ComboBoxItem Content="Mammal N-glycan" FontSize="12"></ComboBoxItem>
            <ComboBoxItem Content="Mammal O-glycan" FontSize="12"></ComboBoxItem>
            <ComboBoxItem Content="Mammal GSL" FontSize="12"></ComboBoxItem>
            <ComboBoxItem Content="Plant N-glycan" FontSize="12"></ComboBoxItem>
            <ComboBoxItem Content="Plant O-glycan" FontSize="12"></ComboBoxItem>
            <ComboBoxItem Content="Fungal N-glycan" FontSize="12"></ComboBoxItem>
            <ComboBoxItem Content="Fungal O-glycan" FontSize="12"></ComboBoxItem>
        </ComboBox>

        <ui:LayoutPanel x:Name="advanced" Grid.RowSpan="5" Grid.Column="2" Visibility="Collapsed" Height="435" VerticalAlignment="Top" Margin="0,190,0,0">

            <ui:ToggleSwitch x:Name="HexNtoggleSwitch" Grid.Row="0" Grid.RowSpan="5" Grid.Column="2" Margin="0,0,0,397" VerticalAlignment="Bottom" OffContent="" OnContent="" Toggled="HexNtoggleSwitch_Toggled" Width="50"/>
            <Label Grid.Row="0" Grid.RowSpan="5" Grid.Column="2" Margin="50,0,0,404" Height="20" VerticalAlignment="Bottom">Hexosamine (HexN)</Label>
            <ui:LayoutPanel x:Name="HexN_container" Margin="0,0,0,401" Visibility="Collapsed" Height="24" VerticalAlignment="Bottom">
                <TextBox x:Name="HexNMin" Grid.Row="0" Grid.RowSpan="5" PreviewTextInput="NumberValidationTextBox" Grid.Column="2" Text="0" MaxLength="2" VerticalAlignment="Center" Margin="0,0,70,0" Width="50" Height="20" MinHeight="0" Padding="10,0,6,0" HorizontalAlignment="Right"/>
                <TextBox x:Name="HexNMax" Grid.Row="0" Grid.RowSpan="5" PreviewTextInput="NumberValidationTextBox" Grid.Column="2" Text="0" MaxLength="2" VerticalAlignment="Center" Width="50" Height="20" MinHeight="0" Padding="10,0,6,0" HorizontalAlignment="Right"/>
            </ui:LayoutPanel>

            <ui:ToggleSwitch x:Name="HexAtoggleSwitch" Grid.Row="0" Grid.RowSpan="5" Grid.Column="2" OffContent="" OnContent="" Toggled="HexAtoggleSwitch_Toggled" Width="50" Margin="0,0,0,370" VerticalAlignment="Bottom"/>
            <Label Grid.Row="0" Grid.RowSpan="5" Grid.Column="2" Margin="50,0,0,377" Height="20" VerticalAlignment="Bottom">Hexuronic acid (HexA)</Label>
            <ui:LayoutPanel x:Name="HexA_container" Grid.Row="0" Grid.RowSpan="5" Grid.Column="2" Margin="0,0,0,363" Visibility="Collapsed" Height="34" VerticalAlignment="Bottom">
                <TextBox x:Name="HexAMin" PreviewTextInput="NumberValidationTextBox" Text="0" MaxLength="2" VerticalAlignment="Top" Margin="0,2,70,0" Width="50" Height="20" MinHeight="0" Padding="10,0,6,0" HorizontalAlignment="Right"/>
                <TextBox x:Name="HexAMax" PreviewTextInput="NumberValidationTextBox" Text="0" MaxLength="2" VerticalAlignment="Top" Margin="0,2,0,0" Width="50" MinHeight="0" Padding="10,0,6,0" HorizontalAlignment="Right"/>
            </ui:LayoutPanel>

            <ui:ToggleSwitch x:Name="dHexNActoggleSwitch" Grid.Row="0" Grid.RowSpan="5" Grid.Column="2" OffContent="" OnContent="" Toggled="dHexNActoggleSwitch_Toggled" Width="50" Margin="0,0,0,343" VerticalAlignment="Bottom"/>
            <Label Grid.Row="0" Grid.RowSpan="5" Grid.Column="2" Margin="50,0,0,350" Height="20" VerticalAlignment="Bottom">N-acetyl-deoxyhexose (dHexNAc)</Label>
            <ui:LayoutPanel x:Name="dHexNAc_container" Margin="0,0,0,342" Visibility="Collapsed" Height="28" VerticalAlignment="Bottom">
                <TextBox x:Name="dHexNAcMin" Grid.Row="0" Grid.RowSpan="5" PreviewTextInput="NumberValidationTextBox" Grid.Column="2" Text="0" MaxLength="2" VerticalAlignment="Top" Margin="0,1,70,0" Width="50" Height="20" MinHeight="0" Padding="10,0,6,0" HorizontalAlignment="Right"/>
                <TextBox x:Name="dHexNAcMax" Grid.Row="0" Grid.RowSpan="5" PreviewTextInput="NumberValidationTextBox" Grid.Column="2" Text="0" MaxLength="2" VerticalAlignment="Top" Margin="0,1,0,0" Width="50" Height="20" MinHeight="0" Padding="10,0,6,0" HorizontalAlignment="Right"/>
            </ui:LayoutPanel>

            <ui:ToggleSwitch x:Name="PenttoggleSwitch" Grid.Row="0" Grid.RowSpan="5" Grid.Column="2" OffContent="" OnContent="" Toggled="PenttoggleSwitch_Toggled" Width="50" Margin="0,0,0,316" VerticalAlignment="Bottom"/>
            <Label Grid.Row="0" Grid.RowSpan="5" Grid.Column="2" Margin="50,0,0,321" Height="22" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="248">Pentose (Pent)</Label>
            <ui:LayoutPanel x:Name="Pent_container" Margin="0,0,0,314" Visibility="Collapsed" Height="29" VerticalAlignment="Bottom">
                <TextBox x:Name="PentMin" Grid.Row="0" Grid.RowSpan="5" PreviewTextInput="NumberValidationTextBox" Grid.Column="2" Text="0" MaxLength="2" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,0,70,0" Width="50" Height="20" MinHeight="0" Padding="10,0,6,0"></TextBox>
                <TextBox x:Name="PentMax" Grid.Row="0" Grid.RowSpan="5" PreviewTextInput="NumberValidationTextBox" Grid.Column="2" Text="0" MaxLength="2" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,0,0,0" Width="50" Height="20" MinHeight="0" Padding="10,0,6,0"></TextBox>
            </ui:LayoutPanel>

            <ui:ToggleSwitch x:Name="KDNtoggleSwitch" Grid.Row="0" Grid.RowSpan="5" Grid.Column="2" Margin="0,0,0,289" VerticalAlignment="Bottom" OffContent="" OnContent="" Toggled="KDNtoggleSwitch_Toggled" Width="50"/>
            <Label Grid.Row="0" Grid.RowSpan="5" Grid.Column="2" Margin="50,0,0,296" Height="20" VerticalAlignment="Bottom">KDN</Label>
            <ui:LayoutPanel x:Name="KDN_container" Margin="0,0,0,289" Visibility="Collapsed" Height="27" VerticalAlignment="Bottom">
                <TextBox x:Name="KDNMin" Grid.Row="0" Grid.RowSpan="5" PreviewTextInput="NumberValidationTextBox" Grid.Column="2" Text="0" MaxLength="2" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,0,70,0" Width="50" Height="20" MinHeight="0" Padding="10,0,6,0"></TextBox>
                <TextBox x:Name="KDNMax" Grid.Row="0" Grid.RowSpan="5" PreviewTextInput="NumberValidationTextBox" Grid.Column="2" Text="0" MaxLength="2" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,0,0,0" Width="50" Height="20" MinHeight="0" Padding="10,0,6,0"></TextBox>
            </ui:LayoutPanel>

            <ui:ToggleSwitch x:Name="PhostoggleSwitch" Grid.Row="0" Grid.RowSpan="5" Grid.Column="2" Margin="0,0,0,262" VerticalAlignment="Bottom" OffContent="" OnContent="" Toggled="PhostoggleSwitch_Toggled" Width="50"/>
            <Label Grid.Row="0" Grid.RowSpan="5" Grid.Column="2" Margin="50,0,0,269" Height="20" VerticalAlignment="Bottom">Phosphate (Phos)</Label>
            <ui:LayoutPanel x:Name="Phos_container" Margin="0,0,0,262" Visibility="Collapsed" Height="27" VerticalAlignment="Bottom">
                <TextBox x:Name="PhosMin" Grid.Row="0" Grid.RowSpan="5" PreviewTextInput="NumberValidationTextBox" Grid.Column="2" Text="0" MaxLength="2" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,0,70,0" Width="50" Height="20" MinHeight="0" Padding="10,0,6,0"></TextBox>
                <TextBox x:Name="PhosMax" Grid.Row="0" Grid.RowSpan="5" PreviewTextInput="NumberValidationTextBox" Grid.Column="2" Text="0" MaxLength="2" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,0,0,0" Width="50" Height="20" MinHeight="0" Padding="10,0,6,0"></TextBox>
            </ui:LayoutPanel>

            <ui:ToggleSwitch x:Name="SulftoggleSwitch" Grid.Row="0" Grid.RowSpan="5" Grid.Column="2" Margin="0,0,0,235" VerticalAlignment="Bottom" OffContent="" OnContent="" Toggled="SulftoggleSwitch_Toggled" Width="50"/>
            <Label Grid.Row="0" Grid.RowSpan="5" Grid.Column="2" Margin="50,0,0,242" Height="20" VerticalAlignment="Bottom">Sulfate (Sulf)</Label>
            <ui:LayoutPanel x:Name="Sulf_container" Margin="0,0,0,235" Visibility="Collapsed" Height="27" VerticalAlignment="Bottom">
                <TextBox x:Name="SulfMin" Grid.Row="0" Grid.RowSpan="5" PreviewTextInput="NumberValidationTextBox" Grid.Column="2" Text="0" MaxLength="2" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,0,70,0" Width="50" Height="20" MinHeight="0" Padding="10,0,6,0"></TextBox>
                <TextBox x:Name="SulfMax" Grid.Row="0" Grid.RowSpan="5" PreviewTextInput="NumberValidationTextBox" Grid.Column="2" Text="0" MaxLength="2" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,0,0,0" Width="50" Height="20" MinHeight="0" Padding="10,0,6,0"></TextBox>
            </ui:LayoutPanel>

            <ui:ToggleSwitch x:Name="lNeuActoggleSwitch" Grid.Row="0" Grid.RowSpan="5" Grid.Column="2" Margin="0,0,0,211" VerticalAlignment="Bottom" OffContent="" OnContent="" Toggled="lNeuActoggleSwitch_Toggled" Width="50"/>
            <Label Grid.Row="0" Grid.RowSpan="5" Grid.Column="2" Margin="50,0,0,217" Height="20" VerticalAlignment="Bottom">Lactonised NeuAc (L)</Label>
            <ui:LayoutPanel x:Name="lNeuAc_container" Margin="0,0,0,210" Visibility="Collapsed" Height="27" VerticalAlignment="Bottom">
                <TextBox x:Name="lNeuAcMin" Grid.Row="0" Grid.RowSpan="5" PreviewTextInput="NumberValidationTextBox" Grid.Column="2" Text="0" MaxLength="2" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,0,70,0" Width="50" Height="20" MinHeight="0" Padding="10,0,6,0"/>
                <TextBox x:Name="lNeuAcMax" Grid.Row="0" Grid.RowSpan="5" PreviewTextInput="NumberValidationTextBox" Grid.Column="2" Text="0" MaxLength="2" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,0,0,0" Width="50" Height="20" MinHeight="0" Padding="10,0,6,0"/>
            </ui:LayoutPanel>

            <ui:ToggleSwitch x:Name="eNeuActoggleSwitch" Grid.Row="0" Grid.RowSpan="5" Grid.Column="2" Margin="0,0,0,186" VerticalAlignment="Bottom" OffContent="" OnContent="" Toggled="eNeuActoggleSwitch_Toggled" Width="50"/>
            <Label Grid.Row="0" Grid.RowSpan="5" Grid.Column="2" Margin="50,0,0,192" Height="20" VerticalAlignment="Bottom">Ethyl Esterified NeuAc (E)</Label>
            <ui:LayoutPanel x:Name="eNeuAc_container" Margin="0,0,0,191" Visibility="Collapsed" Height="21" VerticalAlignment="Bottom">
                <TextBox x:Name="eeNeuAcMin" Grid.Row="0" Grid.RowSpan="5" PreviewTextInput="NumberValidationTextBox" Grid.Column="2" Text="0" MaxLength="2" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,0,70,0" Width="50" Height="20" MinHeight="0" Padding="10,0,6,0"/>
                <TextBox x:Name="eeNeuAcMax" Grid.Row="0" Grid.RowSpan="5" PreviewTextInput="NumberValidationTextBox" Grid.Column="2" Text="0" MaxLength="2" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,0,0,0" Width="50" Height="20" MinHeight="0" Padding="10,0,6,0"/>
            </ui:LayoutPanel>

            <ui:ToggleSwitch x:Name="dNeuActoggleSwitch" Grid.Row="0" Grid.RowSpan="5" Grid.Column="2" Margin="0,0,0,161" VerticalAlignment="Bottom" OffContent="" OnContent="" Toggled="dNeuActoggleSwitch_Toggled" Width="50"/>
            <Label Grid.Row="0" Grid.RowSpan="5" Grid.Column="2" Margin="50,0,0,167" Height="20" VerticalAlignment="Bottom">Diethylamidated NeuAc (D)</Label>
